Host me
    Hostname localhost
    LocalForward 2222 localhost:22
    GatewayPorts yes

Host nuc
    User om
    HostName 192.168.10.180
    ControlPath ~/.ssh/ctrl-%l-to-%h
    ControlMaster auto
    ControlPersist 10m
    #ControlPersist yes
    ServerAliveInterval 180
    # so that others can connect
    GatewayPorts true
    DynamicForward 9999
    # squid
    LocalForward 3128 localhost:3128
    # data
    LocalForward 2282 192.168.10.182:22
    # dimpi-air
    LocalForward 2220 192.168.10.160:2220
    # anaya-air
    LocalForward 2221 192.168.10.160:2221

