#!/bin/bash
if [[ ! -d ~/gcode ]]; then
  cd
  git clone https://github.com/random220/gcode
  rm -f .profile .bashrc .aliases .vimrc .tmux.conf
  cat <<'EOF' >.profile
if [[ -f ~/.bashrc ]]; then
  . ~/.bashrc
fi
EOF
  ln -s gcode/sundry/.bashrc
  ln -s gcode/sundry/.aliases
  ln -s gcode/sundry/.vimrc
  ln -s gcode/sundry/.tmux.conf 
  . ~/.bashrc
fi


