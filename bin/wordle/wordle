#!/usr/bin/python3
import random
import sys

def main():
    words_list = list(G.words_dict.keys())

    n = len(words_list)
    i = random.randint(0, n-1)
    G.word = words_list[i]
    G.word_hash = breakout(G.word)
    
    while not guess_right():
        pass

def guess_right():
    inw = sys.stdin.readline().strip().lower()
    if inw == 'h' or inw == 'q' or inw == '.':
        print(G.word.upper())
        sys.exit(0)
        return('')
    if inw not in G.words_dict:
        print('Not in dictionary')
        return('')

    n = breakout(inw)
    x = ''
    i = 0
    for c in inw:
        i += 1
        if c in G.word_hash:
            if i in G.word_hash[c]:
                x = x+c.upper()
                G.leftover = G.leftover.replace(c.lower(), c.upper())
            else:
                x = x+c.lower()
                G.leftover = G.leftover.replace(c.upper(), c.lower())
        else:
            x = x+'*'
            G.leftover = G.leftover.replace(c.lower(), '.')
    print('=> '+x)
    print()

    if inw == G.word:
        return True

    print(G.leftover)
    return False

def breakout(w):
    word = {}
    n = 1
    for c in w:
        c = c.lower()
        if c not in word:
            word[c] = {}
        word[c][n] = True
        n = n+1
    return word

