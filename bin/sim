#!/bin/bash
f="$1"
if [[ "$f" == '' ]]; then
    f=$(
        ls -1tr \
          ~/Desktop/x/Portfolio_Positions*csv \
          ~/Desktop/Portfolio_Positions*csv \
          ~/Downloads/Portfolio_Positions*csv \
          ~/Downloads/x/Portfolio_Positions*csv \
          2>/dev/null \
        |tail -1
      )
fi
if [[ -f "$f" ]]; then
    f=$(readlink -f "$f")
    cd ~/gcode/fin && ./syms "$f"
    echo
    echo
    echo "rm -f '$f'"
fi

