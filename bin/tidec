#!/bin/bash

cd ~/b/om
rm -rf __xxx
rm -rf __chromecache

genc -t __xxx -d tiddly.html.gpg
mv __xxx/tiddly.html.gpg.plain __xxx/tiddly.html

(
'/Applications/Google Chrome.app/Contents/MacOS/Google Chrome' --user-data-dir=./__chromecache __xxx/tiddly.html >/dev/null 2>&1
) >/dev/null 2>&1 &
sleep 3

rm -rf __xxx
